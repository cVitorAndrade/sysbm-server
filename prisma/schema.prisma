// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Librarian {
  id          String @id
  email       String @unique
  name        String
  password    String
  permission  String
  cpf         String
  phoneNumber String
  status      String
  createdAt   DateTime
}
